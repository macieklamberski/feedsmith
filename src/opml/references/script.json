              {
                "text": "Change 'output' to 'export'."
              }
            ]
          },
          {
            "text": "25/01/2024; 3:22:15 PM by AS",
            "isComment": true,
            "outlines": [
              {
                "text": "Create test data by generating JSON files with random user information."
              }
            ]
          }
        ]
      },
      {
        "text": "on createUserData (filename, count)",
        "outlines": [
          {
            "text": "directory.ensurePath (filename)",
            "isBreakpoint": true
          },
          {
            "text": "file.writeJsonFile (filename, string.generateRandomUsers (count))"
          }
        ]
      },
      {
        "text": "local (outputDir = user.app.preferences.dataFolder + 'test\\\\users\\\\')"
      },
      {
        "text": "for i = 1 to 10",
        "outlines": [
          {
            "text": "createUserData (outputDir + 'batch' + i + '.json', random (50, 200))"
          }
        ]
      }
    ]
  }
}
