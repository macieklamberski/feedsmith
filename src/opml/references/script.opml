  <body>
    <outline text="Changes" isComment="false">
      <outline text="18/02/2024; 9:15:22 AM by AS">
        <outline text="Change 'output' to 'export'."/>
      </outline>
      <outline text="25/01/2024; 3:22:15 PM by AS" isComment="true">
        <outline text="Create test data by generating JSON files with random user information."/>
      </outline>
    </outline>
    <outline text="on createUserData (filename, count)">
      <outline text="directory.ensurePath (filename)" isBreakpoint="true"/>
      <outline text="file.writeJsonFile (filename, string.generateRandomUsers (count))"/>
    </outline>
    <outline text="local (outputDir = user.app.preferences.dataFolder + 'test\\users\\')"/>
    <outline text="for i = 1 to 10">
      <outline text="createUserData (outputDir + 'batch' + i + '.json', random (50, 200))"/>
    </outline>
  </body>
</opml>
